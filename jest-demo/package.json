{
  "name": "@repris/jest-demo",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "files": [
    "package.json",
    "lib",
    ".tsc"
  ],
  "type": "module",
  "devDependencies": {
    "@types/jest": "^28",
    "@types/node": "^16",
    "@repris/jest": "workspace:*",
    "@repris/samplers": "workspace:*",
    "@repris/util": "workspace:*",
    "jest": "^28",
    "typescript": "~4.9",
    "vite": "~2.9"
  },
  "scripts": {
    "build:tsc": "tsc --build tsconfig.json",
    "build": "build:tsc",
    "test": "NODE_OPTIONS='--experimental-vm-modules -C development' node --expose-gc ./node_modules/jest/bin/jest.js",
    "test:debug": "NODE_OPTIONS='--experimental-vm-modules -C development' node --expose-gc --inspect-brk ./node_modules/jest/bin/jest.js --runInBand"
  },
  "license": "MIT"
}
